package Power;
import java.util.Scanner;
public class Power2
{
  public static void main(String []args)
  {
	  Scanner sc = new Scanner(System.in);
	  System.out.println("Enter the Number");
	  int n=sc.nextInt();
	  if(n<31)
	  {
		  int sum =2;
		  for(int i=1;i<=n;i++)
		  {
			  sum=sum*i;
			  System.out.println("2 "+"*" + "=" + sum);
			  }
		  }
	  }
}
